server.ssr.schedules=0 0/30 * * * ?
spring.application.name=nasi-pay
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/ssr?serverTimezone=Europe/Paris&autoReconnect=true&useUnicode=true&characterEncoding=utf-8
spring.datasource.username=
